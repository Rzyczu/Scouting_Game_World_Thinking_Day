{
  "expo": {
    "name": "DMB",
    "slug": "rzyczu-dmb",
    "description": "Aplikacja stworzona na potrzebę zbiórki harcerskiej DMB. 3PGDH Tarcza.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rzyczu.DMB",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.rzyczu.DMB",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}